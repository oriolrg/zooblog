{
    "name": "vanilo/product",
    "description": "Vanilo Product Module",
    "type": "library",
    "license": "MIT",
    "keywords": ["product", "ecommerce", "vanilo", "artkonekt", "laravel"],
    "support": {
        "source": "https://github.com/artkonekt/product"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "konekt/concord": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "6.3 - 7.0",
        "orchestra/testbench": "~3.4",
        "cviebrock/eloquent-sluggable": "~4.2"
    },
    "autoload": {
        "psr-4": { "Vanilo\\Product\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Product\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
